# Anthropic API key (required)
ANTHROPIC_API_KEY=sk-ant-xxx

# Database path (optional, default: ./data/reasoning.db)
DATABASE_PATH=./data/reasoning.db

# Log level (optional, default: info)
# Options: error, warn, info, debug, trace
LOG_LEVEL=info

# Request timeout in milliseconds (optional, default: 30000)
REQUEST_TIMEOUT_MS=30000

# Maximum retries for API calls (optional, default: 3)
MAX_RETRIES=3

# MCP transport type (optional, default: stdio)
# Options: stdio, http
MCP_TRANSPORT=stdio
